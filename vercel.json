{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt|google.*\\.html|vite\\.svg|images/.*).*)",
      "destination": "/index.html"
    }
  ]
}

