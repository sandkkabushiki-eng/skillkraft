{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "redirects": [
    {
      "source": "https://skillkraft.xyz/:path*",
      "destination": "https://www.skillkraft.xyz/:path*",
      "permanent": true
    },
    {
      "source": "https://skillkraft.xyz",
      "destination": "https://www.skillkraft.xyz",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

